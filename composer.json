{
    "name": "solital/katrina",
    "description": "Official Solital ORM for database abstraction",
    "license": "MIT",
    "keywords": [
        "orm",
        "php",
        "database",
        "mysql",
        "abstraction",
        "pdo"
    ],
    "autoload": {
        "psr-4": {
            "Katrina\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "symfony/uid": "^7.1"
    },
    "autoload-dev": {
        "psr-4": {
            "KatrinaTest\\": "tests/"
        },
        "files": [
            "tests/configTest.php"
        ]
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^11",
        "symfony/var-dumper": "^7.1"
    }
}
