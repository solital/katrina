{
    "name": "solital/katrina",
    "description": "Official Solital ORM for database abstraction",
    "keywords": [
        "orm",
        "php",
        "database",
        "mysql",
        "abstraction",
        "pdo"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Katrina\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-pdo": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "KatrinaTest\\": "tests/"
        },
        "files": [
            "tests/configTest.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}